    "handlebars": "^4.7.7",
    "jest": "^29.5.0",
    "lodash": "^4.17.21",
    "nyc": "^15.1.0",
    "tryrequire": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "mongodb": "^5.6.0"
  },
